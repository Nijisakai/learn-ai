{
  "gitbook": "3.2.2",
  "description": "learn ai",
    "plugins": [
		"chapter-fold",
		"code",
		"latex-codecogs",
		"-highlight",
		"tbfed-pagefooter",
        "flexible-alerts",
        "js-sequence-diagram-full@>=0.3.1",
		"-sharing"
	],

    "pluginsConfig": {
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 1
        },
		"tbfed-pagefooter": {
            "copyright": "Copyright &copy; 北京师范大学智慧学习研究院",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
	
}
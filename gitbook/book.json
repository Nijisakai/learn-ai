{
  "gitbook": "3.2.2",
  "description": "learn ai",
    "plugins": [
		"chapter-fold",
        "code",
        "back-to-top-button",
        "splitter",
        "-lunr",
        "-search",
        "search-pro",
        "pageview-count",
        "hide-element",
        "popup",
        "sharing-plus",
        "latex-codecogs",
        "lightbox",
        "expandable-chapters-small",
		"-highlight",
		"tbfed-pagefooter",
        "flexible-alerts",
        "js-sequence-diagram-full@>=0.3.1",
		"-sharing"
	],

    "pluginsConfig": {
        "hide-element": {
            "elements": [".gitbook-link"]
        },
		"tbfed-pagefooter": {
            "copyright": "&copy 北京师范大学智慧学习研究院",
            "modify_label": "修订时间：",
            "modify_format": "YYYY-MM-DD"
        },
        "sharing": {
            "douban": true,
            "facebook": false,
            "google": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "twitter": false,
            "weibo": true,
            "all": [
                "douban",
                "qq",
                "qzone"
            ]
        }
    }
	
}
